{
  "name": "i18n-extract-utils",
  "version": "0.11.5",
  "description": "React/Vue i18n extraction and transformation utility",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "i18n-extract": "dist/cli.js"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "node dist/cli.js",
    "dev": "ts-node --esm src/cli.ts",
    "test": "vitest run",
    "test:dist": "node __tests__/dist-test.js",
    "test:all": "pnpm test && pnpm test:dist",
    "prepublishOnly": "pnpm build && pnpm test:all && pnpm lint",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts"
  },
  "keywords": [
    "i18n",
    "react",
    "vue",
    "internationalization",
    "ast"
  ],
  "author": "Dz",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/generator": "^7.27.0",
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "fast-glob": "^3.3.3"
  },
  "optionalDependencies": {},
  "peerDependencies": {
    "@vue/compiler-sfc": "^3.0.0",
    "commander": "^9.0.0 || ^10.0.0",
    "vue-template-compiler": "^2.6.0"
  },
  "peerDependenciesMeta": {
    "@vue/compiler-sfc": {
      "optional": true
    },
    "commander": {
      "optional": true
    },
    "vue-template-compiler": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.20.4",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=14.17"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}