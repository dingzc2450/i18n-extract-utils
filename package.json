    "glob": "^10.3.10"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.4",
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vitest": "^3.1.1"
  }
