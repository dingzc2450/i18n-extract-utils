{
  "name": "i18n-extract-utils",
  "version": "0.0.3",
  "description": "React i18n extraction and transformation utility",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "i18n-extract": "dist/cli.js"
  },
